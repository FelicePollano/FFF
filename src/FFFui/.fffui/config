[extension]

open-cmd="%programfiles%\\Microsoft VS Code\\Code.exe" 
open-cmdline=%file%
open-atline-cmd="%programfiles%\\Microsoft VS Code\\Code.exe" 
open-atline-cmdline="--goto \"%file%\":%line%:1"

[extension "cs,cpp,h,rc,c,vb,xaml,xml,xslt,sql"]

open-cmd="%programfiles%\\Microsoft Visual Studio\\2022\\Professional\\Common7\\IDE\\devenv.exe"
open-cmdline="/edit \"%file%\""
open-atline-cmd="%programfiles%\\Microsoft Visual Studio\\2022\\Professional\\Common7\\IDE\\devenv.exe"
open-atline-cmdline="/edit \"%file%\" /command \"edit.goto %line%\""

[compare]
cmd="%programfiles%\\KDiff3\\kdiff3.exe"
cmdline="\"%file1%\" \"%file2%\""
